{
  "name": "js-paint",
  "version": "0.1.0",
  "description": "Simple paint app made with front-end technologies",
  "main": "src/main.ts",
  "author": "Carlos Sanz Garcia <studiocam6@yahoo.es>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --progress --content-base ./dist",
    "build": "webpack",
    "test": "jest"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [ "ts", "tsx", "js", "jsx", "json" ]
  }
}
