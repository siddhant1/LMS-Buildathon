{
  "name": "js-paint",
  "version": "0.1.0",
  "description": "Simple paint app made with front-end technologies",
  "main": "src/main.ts",
  "author": "Carlos Sanz Garcia <studiocam6@yahoo.es>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --progress --content-base ./dist",
    "build": "webpack",
    "test": "jest --no-cache --verbose",
    "lint": "tslint -p tslint.json"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {
    "@types/events": "^1.1.0",
    "@types/paper": "^0.9.9",
    "events": "^1.1.1",
    "nodelist-foreach-polyfill": "^1.2.0",
    "normalize-scss": "^7.0.0",
    "paper": "^0.11.5"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "CanvasComponent.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
